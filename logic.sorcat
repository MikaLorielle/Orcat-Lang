pub fn and(bool a, bool b) <bool> {
    if (a) {
        if (b) {
            return true;
        } else {
            return false;
        }
    } else {
        return false;
    }
}

pub fn or(bool a, bool b) <bool> {
    if (a) {
        return true;
    } else if (b) {
        return true;
    } else {
        return false;
    }
}
